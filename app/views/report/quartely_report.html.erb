<head>
  <title>Report</title>

  <script type="text/javascript" >

    var start_date = '<%= @start_date %>';
    var end_date = '<%= @end_date %>';

    function loadReportData() {
      jQuery.ajax({
        type: "POST",
        url: "/get_quartely_report_data",
        data: "start_date=" + start_date + "&end_date=" + end_date,
        beforeSend: function(){
          jQuery(".valueCell").html('<img src="/images/loader.gif"/>');
        },

        success: function(result){
          result = JSON.parse(result);
          age_1 = result["age"]["0_1"];
          age_2 = result["age"]["1_14"];
          age_3 = result["age"]["15_24"];
          age_4 = result["age"]["25_49"];
          age_5 = result["age"]["50_above"];
          age_total = age_1.length + age_2.length + age_3.length + age_4.length + age_5.length;

          jQuery(".age-1").html(age_1.length);
          jQuery(".age-2").html(age_2.length);
          jQuery(".age-3").html(age_3.length);
          jQuery(".age-4").html(age_4.length);
          jQuery(".age-5").html(age_5.length);

          jQuery(".age-category-total").html(age_total)
          console.log(result)
        
        }

      });

    }

    function loadHIVstatusData(){
      jQuery.ajax({
        type: "POST",
        url: "/get_hiv_data",
        data: "start_date=" + start_date + "&end_date=" + end_date,
        beforeSend: function(){
          //jQuery(".valueCell").html('<img src="/images/loader.gif"/>');
        },

        success: function(result){
          result = JSON.parse(result);

          prev_positive_not_on_art = result["prev_positive_not_on_art"]
          prev_positive_on_art = result["prev_positive_on_art"]
          new_negatives = result["new_negatives"]
          new_positives = result["new_positives"]
          testing_declined = result["testing_declined"]
          testing_not_done = result["testing_not_done"]

          total_eligible_clients = prev_positive_not_on_art.length + prev_positive_on_art.length + new_negatives.length + new_positives.length + testing_declined.length + testing_not_done.length;

          jQuery(".previous_positive_not_on_art").html(prev_positive_not_on_art.length);
          jQuery(".previous_positive_on_art").html(prev_positive_on_art.length);
          jQuery(".new_negatives").html(new_negatives.length);
          jQuery(".new_positives").html(new_positives.length);
          jQuery(".test_declined").html(testing_declined.length);
          jQuery(".test_not_done").html(testing_not_done.length);

          jQuery(".total_eligible_clients").html(total_eligible_clients);
          console.log(result)

        }

      });
    }

