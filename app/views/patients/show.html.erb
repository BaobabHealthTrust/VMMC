
<script type="text/javascript">
  var patientID = "<%= params[:patient_id] %>";
  var selectedEncounterID = "";

  function cancel(){
    window.location = "/";
  }

  function finish(){
    window.location = "/";
  }

  function editDemographics(){
    
  }

  function goToEncouterType(encounter_type){
    window.location = "/encounters/new?encounter_type=" + encounter_type + "&patient_id=" + patientID;
  }


  function getPatientVisits() {
    jQuery.ajax({
      type: "POST",
      url: "/patients/get_patient_visits",
      data: "patient_id=" + patientID,
      success: function(result){
        result = JSON.parse(result);
        result = sortObjByDate(result);
        ulVisits = document.getElementById("ulVisits");

        for(var i=0; i<result.length; i++){
          encounter_date = formatDate(new Date(result[i]));
          li = '<li onclick="showEncounterDetails(&quot;' + encounter_date + '&quot;); setClassName(this)" style="padding: 15px 10px; cursor: pointer; margin-bottom: 5px; border-bottom: 1px solid rgb(204, 204, 204); text-align: center;" >' + encounter_date + '</li>';
          ulVisits.innerHTML += li;
        }

        jQuery("#ulVisits li:first-child").click();
      }

    });

  }

  function sortObjByDate(result) {
    encounter_dates = []
    for (var encounterID in result){
      encounter_date = result[encounterID]["encounter_date"];
      encounter_date = new Date(encounter_date);
      encounter_dates.push(encounter_date.getTime())
    }
    return encounter_dates.sort().reverse(); //Latest date first

  }

  function formatDate(date) {
    var monthNames = [
      "Jan", "Feb", "Mar",
      "Apr", "May", "Jun", "Jul",
      "Aug", "Sep", "Oct",
      "Nov", "Dec"
    ];

    var day = date.getDate();
    if (day < 10){
      day = "0" + day;
    }
    var monthIndex = date.getMonth();
    var year = date.getFullYear();

    return day + '/' + monthNames[monthIndex] + '/' + year;
  }

  function setClassName(obj){
    jQuery("li").removeClass("active-link");
    obj.className = "active-link";
  }

  function showEncounterDetails(encounter_date){
    encounterIframe = document.getElementById("encounter-data");
    encounterIframe.src = "";
    encounterIframe.src = "/encounters/details?encounter_date=" + encounter_date + "&patient_id=" + patientID;
  }

  function loadVitals(){
    jQuery.ajax({
      type: "POST",
      url: "/patients/get_patient_vitals",
      data: "patient_id=" + patientID,
      success: function(result){
        result = JSON.parse(result);
        sbp = result["sbp"];
        if (!sbp) sbp = "?";
        dbp = result["dbp"];
        if (!dbp) dbp = "?";
        bmi = result["bmi"];
        if (!bmi) bmi = "?";
        temp = result["temp"];
        if (!temp) temp = "?";
        weight = result["weight"];
        if (!weight) weight = "?";
        height = result["height"];
        if (!height) height = "?";

        document.getElementById("bp").innerHTML = sbp + "/" + dbp;
        document.getElementById("weight").innerHTML = weight;
        document.getElementById("bmi").innerHTML = bmi;
        document.getElementById("height").innerHTML = height;
        document.getElementById("temperature").innerHTML = temp;
      }

    });
  }

  function loadActivities(){
    encounterIframe = document.getElementById("encounter-data");
    encounterIframe.src = "";
    encounterIframe.src = "/patients/activities?patient_id=" + patientID
  }
  
  window.onload = function(){
    getPatientVisits();
    loadVitals();
    checkIfPatientIsFollowUp();
  }

  function checkIfPatientIsFollowUp(){
    jQuery.ajax({
      type: "POST",
      url: "/patients/get_follow_up_status",
      data: "patient_id=" + patientID,
      success: function(result){
        if (result == 'true'){
          jQuery("#registration").hide();
          jQuery("#medical-history").hide();
          jQuery("#vital-signs").hide();
          jQuery("#hiv-status").hide();
          jQuery("#genital-examination").hide();
          jQuery("#circumcision").hide();
          jQuery("#post_op_review").hide();
          checkIfEncounterCaptured('FOLLOW UP', 'follow_up_review');
        } else {
          jQuery("#follow_up_review").hide();
          checkIfPatientHasRegistrationEncounter();
          checkIfEncounterCaptured('MEDICAL HISTORY', 'medical-history');
          checkIfEncounterCaptured('VITALS', 'vital-signs');
          checkIfEncounterCaptured('HIV TESTING', 'hiv-status');
          checkIfEncounterCaptured('GENITAL EXAMINATION', 'genital-examination');
          checkIfEncounterCaptured('CIRCUMCISION', 'circumcision');
          checkIfEncounterCaptured('POST-OP REVIEW', 'post_op_review');
        }
      }

    });
  }

  function checkIfPatientHasRegistrationEncounter(){
    jQuery.ajax({
      type: "POST",
      url: "/patients/get_registration_encounter_status",
      data: "patient_id=" + patientID,
      success: function(result){
        if (result == 'true'){
          jQuery("#registration").hide();
        }
      }

    });
  }

  function checkIfPatientIsCircumcised(){
    jQuery.ajax({
      type: "POST",
      url: "/patients/patient_is_circumcised",
      data: "patient_id=" + patientID,
      success: function(result){
        if (result == 'false'){
          jQuery("#post_op_review").hide();
          jQuery("#follow_up_review").hide();
        }
      }

    });
  }

  function checkIfPatientIsCircumcisedToday(){
    jQuery.ajax({
      type: "POST",
      url: "/patients/patient_is_circumcised_today",
      data: "patient_id=" + patientID,
      success: function(result){
        if (result == 'true'){
          /*jQuery("#follow_up_review").hide();
          checkIfEncounterCaptured('medical');
          checkIfEncounterCaptured('vital');
          checkIfEncounterCaptured('hiv');
          checkIfEncounterCaptured('genital');
          checkIfEncounterCaptured('circumcision');
          checkIfEncounterCaptured('post');*/
        }
      }

    });
  }

  function checkIfEncounterCaptured(encounterType, id){
    jQuery.ajax({
      type: "POST",
      url: "/patients/check_if_encounter_exists_on_date",
      data: "patient_id=" + patientID + "&encounter_type=" + encounterType,
      success: function(result){
        if (result == 'true'){
          jQuery("#" + id).css('color', 'gray');
          jQuery("#" + id).removeAttr("onclick");
          jQuery("#" + id).mouseover(function(){
            jQuery(this).css('background-color', 'white');
          });
        }
      }

    });
  }

  function confirmAction(){
    jQuery.ajax({
      type: "POST",
      url: "/encounters/void",
      data: "encounter_id=" + selectedEncounterID,
      success: function(){
        hidePopup();
        jQuery('.active-link').click();
      }

    });
  }

  function showPopup(){
    jQuery("#cover").show();
    jQuery(".confirm").show();

  }

  function hidePopup(){
    jQuery("#cover").hide();
    jQuery(".confirm").hide();
  }
</script>

